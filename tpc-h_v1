
CREATE TABLE CUSTOMER
(
	C_CUSTKEY int NOT NULL ,
	C_NAME varchar(25) NULL ,
	C_ADDRESS varchar(40) NULL ,
	C_NATIONKEY int NULL ,
	C_PHONE char(15) NULL ,
	C_ACCTBAL decimal(15,2) NULL ,
	C_MKTSEGMENT char(10) NULL ,
	C_COMMENT varchar(117) NULL 
);

CREATE TABLE LINEITEM
(
	L_ORDERKEY int NOT NULL ,
	L_PARTKEY int NULL ,
	L_SUPPKEY int NULL ,
	L_LINENUMBER int NOT NULL ,
	L_QUANTITY decimal(15,2) NULL ,
	L_EXTENDEDPRICE decimal(15,2) NULL ,
	L_DISCOUNT decimal(15,2) NULL ,
	L_TAX decimal(15,2) NULL ,
	L_RETURNFLAG char(1) NULL ,
	L_LINESTATUS char(1) NULL ,
	L_SHIPDATE datetime NULL ,
	L_COMMITDATE datetime NULL ,
	L_RECEIPTDATE datetime NULL ,
	L_SHIPINSTRUCT char(25) NULL ,
	L_SHIPMODE char(10) NULL ,
	L_COMMENT varchar(44) NULL 
);

CREATE TABLE NATION
(
	N_NATIONKEY int NOT NULL ,
	N_NAME char(25) NULL ,
	N_REGIONKEY int NULL ,
	N_COMMENT varchar(152) NULL 
);

CREATE TABLE ORDERS
(
	O_ORDERKEY int NOT NULL ,
	O_CUSTKEY int NULL ,
	O_ORDERSTATUS char(1) NULL ,
	O_TOTALPRICE decimal(15,2) NULL ,
	O_ORDERDATE datetime NULL ,
	O_ORDERPRIORITY char(15) NULL ,
	O_CLERK char(15) NULL ,
	O_SHIPPRIORITY int NULL ,
	O_COMMENT varchar(79) NULL 
);

CREATE TABLE PART
(
	P_PARTKEY int NOT NULL ,
	P_NAME varchar(55) NULL ,
	P_MFGR char(25) NULL ,
	P_BRAND char(10) NULL ,
	P_TYPE varchar(25) NULL ,
	P_SIZE int NULL ,
	P_CONTAINER char(10) NULL ,
	P_RETAILPRICE decimal(15,2) NULL ,
	P_COMMENT varchar(23) NULL 
);

CREATE TABLE PARTSUPP
(
	PS_PARTKEY int NOT NULL ,
	PS_SUPPKEY int NOT NULL ,
	PS_AVAILQTY int NULL ,
	PS_SUPPLYCOST decimal(15,2) NULL ,
	PS_COMMENT varchar(199) NULL 
);

CREATE TABLE REGION
(
	R_REGIONKEY int NOT NULL ,
	R_NAME char(25) NULL ,
	R_COMMENT varchar(152) NULL 
);

CREATE TABLE SUPPLIER
(
	S_SUPPKEY int NOT NULL ,
	S_NAME char(25) NULL ,
	S_ADDRESS varchar(40) NULL ,
	S_NATIONKEY int NULL ,
	S_PHONE char(15) NULL ,
	S_ACCTBAL decimal(15,2) NULL ,
	S_COMMENT varchar(101) NULL 
);

ALTER TABLE CUSTOMER
	ADD CONSTRAINT XPKCUSTOMER PRIMARY KEY NONCLUSTERED (C_CUSTKEY ASC) NOT ENFORCED ;

ALTER TABLE LINEITEM
	ADD CONSTRAINT XPKLINEITEM PRIMARY KEY NONCLUSTERED (L_ORDERKEY ASC,L_LINENUMBER ASC) NOT ENFORCED ;

ALTER TABLE NATION
	ADD CONSTRAINT XPKNATION PRIMARY KEY NONCLUSTERED (N_NATIONKEY ASC) NOT ENFORCED ;

ALTER TABLE ORDERS
	ADD CONSTRAINT XPKORDERS PRIMARY KEY NONCLUSTERED (O_ORDERKEY ASC) NOT ENFORCED ;

ALTER TABLE PART
	ADD CONSTRAINT XPKPART PRIMARY KEY NONCLUSTERED (P_PARTKEY ASC) NOT ENFORCED ;

ALTER TABLE PARTSUPP
	ADD CONSTRAINT XPKPARTSUPP PRIMARY KEY NONCLUSTERED (PS_PARTKEY ASC,PS_SUPPKEY ASC) NOT ENFORCED ;

ALTER TABLE REGION
	ADD CONSTRAINT XPKREGION PRIMARY KEY NONCLUSTERED (R_REGIONKEY ASC) NOT ENFORCED ;

ALTER TABLE SUPPLIER
	ADD CONSTRAINT XPKSUPPLIER PRIMARY KEY NONCLUSTERED (S_SUPPKEY ASC) NOT ENFORCED ;
